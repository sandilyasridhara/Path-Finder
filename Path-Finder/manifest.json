{
  "manifest_version": 3,
  "name": "New_ Genius Search",
  "version": "5.0",
  "icons": {
    "16": "icons/icon16.png"
    
   
  },
  "action": {
    "default_icon": {
      "16": "icons/icon16.png"
      
     
    }
  },
  
  "background": {
  "service_worker": "background.js"
},
  "permissions": [
    "storage","tabs"
  ],
"web_accessible_resources": [
  {
    "resources": [
      "icons/icon16.png",

      "icons/pathfinder.png"
    ],
    "matches": ["<all_urls>"]
  }
],


  "content_scripts": [
    {
      "matches": [
        "https://support.servicenow.com/sn_customerservice_case.do*",
        "https://support.servicenow.com/nav_to.do?uri=sn_customerservice_case.do*",
        "https://support.servicenow.com/sn_customerservice_task.do*",
        "https://support.servicenow.com/nav_to.do?uri=sn_customerservice_task.do*"
      ],
      "js": ["jquery.min.js", "script.js", "modal.js", "modalScript.js", "content.js"],
      "all_frames": true,
      "run_at": "document_end"
    }
  ],
  "options_page": "options.html",
  "host_permissions": ["<all_urls>"]
}
